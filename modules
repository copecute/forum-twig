 <h2><span style='padding: 0 7px 0 0; border-right: 1px solid #ddd'><i class="fa fa-pencil" aria-hidden="true"></i> Chatbox</span><a onclick="if(document.getElementById('bbcode') .style.display=='none') {document.getElementById('bbcode') .style.display=''}else{document.getElementById('bbcode') .style.display='none'}"><span style='margin:0 0 0 8px'>BBcode</span></a></h2>
<script>
function tag(text1, text2) {
if ((document.selection)) {
document.form.comment.focus();
document.form.document.selection.createRange().text = text1+document.form.document.selection.createRange().text+text2;
} else if(document.forms['form'].elements['nd'].selectionStart!=undefined) {
var element = document.forms['form'].elements['nd'];
var str = element.value;
var start = element.selectionStart;
var length = element.selectionEnd - element.selectionStart;
element.value = str.substr(0, start) + text1 + str.substr(start, length) + text2 + str.substr(start + length);
} else {
document.form.nd.value += text1+text2;
}
}
function show_hide(elem) {
obj = document.getElementById(elem);
if( obj.style.display == "none" ) {
obj.style.display = "block";
} else {
obj.style.display = "none";
}
}
</script>
